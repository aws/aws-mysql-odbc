        <Condition Action="show">NOT Installed And ACTION="INSTALL" And $ServerInstanceConfig=3</Condition>
      </Control>
      <Control Id="InstallServiceDescLbl" Type="Text" X="148" Y="100" Width="208" Height="48" Hidden="yes" Transparent="yes">
